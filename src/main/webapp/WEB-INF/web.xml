<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_3_1.xsd"
    version="3.1">
	<display-name>Archetype Created Web Application</display-name>

	

	<servlet>
		<servlet-name>ListConsolasMarcasServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.ListConsolasMarcasServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ListConsolasMarcasServlet</servlet-name>
		<url-pattern>/mostrarlistado</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>ListVideoJuegosMarcasServlet</servlet-name>
		<servlet-class>es.salesianos.servlet.ListVideoJuegosMarcasServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ListVideoJuegosMarcasServlet</servlet-name>
		<url-pattern>/mostrarlistado</url-pattern>
	</servlet-mapping>
	
	<servlet>
        <servlet-name>ListVideoJuegosServlet</servlet-name>
        <servlet-class>es.salesianos.servlet.ListVideoJuegosServlet</servlet-class>
    </servlet>
	
	
	<servlet-mapping>
        <servlet-name>ListVideoJuegosServlet</servlet-name>
        <url-pattern>/cargarListadoVideojuego</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ListEdadRecomendadaServlet</servlet-name>
        <servlet-class>es.salesianos.servlet.ListEdadRecomendadaServlet</servlet-class>
    </servlet>
	
	
	<servlet-mapping>
        <servlet-name>ListEdadRecomendadaServlet</servlet-name>
        <url-pattern>/cargarListadoEdad</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>ListConsolasServlet</servlet-name>
        <servlet-class>es.salesianos.servlet.ListConsolasServlet</servlet-class>
    </servlet>
	
	
	<servlet-mapping>
        <servlet-name>ListConsolasServlet</servlet-name>
        <url-pattern>/cargarListadoConsola</url-pattern>
    </servlet-mapping>
    

	<welcome-file-list>
		<welcome-file>AltaConsola.jsp</welcome-file>
	</welcome-file-list>

</web-app>
